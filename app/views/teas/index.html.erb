<p id="notice"><%= notice %></p>

<h1>Listing Teas</h1>

<table>
  <thead>
    <tr>
      <th>Type</th>
      <th>Quantity</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @teas.each do |tea| %>
      <tr>
        <td><%= tea.type %></td>
        <td><%= tea.quantity %></td>
        <td><%= tea.user_id %></td>
        <td><%= link_to 'Show', tea %></td>
        <td><%= link_to 'Edit', edit_tea_path(tea) %></td>
        <td><%= link_to 'Destroy', tea, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tea', new_tea_path %>
